{
  "name": "my-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server": "npx nodemon --watch \"**\" --ext \"ts,json\" --ignore \"**/*.spec.ts\" --exec \"ts-node-esm server.ts\"",
